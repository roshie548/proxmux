{
  "name": "proxmux",
  "version": "0.4.0",
  "description": "Terminal UI for managing Proxmox VE",
  "author": "Roshan Kumaraswamy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/roshie548/proxmux.git"
  },
  "homepage": "https://github.com/roshie548/proxmux",
  "bugs": {
    "url": "https://github.com/roshie548/proxmux/issues"
  },
  "keywords": [
    "proxmox",
    "terminal",
    "tui",
    "cli",
    "virtualization",
    "lxc",
    "containers",
    "vm",
    "management"
  ],
  "type": "module",
  "bin": {
    "proxmux": "./src/index.tsx"
  },
  "scripts": {
    "start": "bun run src/index.tsx",
    "dev": "bun --watch run src/index.tsx",
    "build": "bun build ./src/index.tsx --compile --minify --outfile dist/proxmux",
    "build:darwin-arm64": "bun build ./src/index.tsx --compile --minify --target=bun-darwin-arm64 --outfile dist/proxmux-darwin-arm64",
    "build:darwin-x64": "bun build ./src/index.tsx --compile --minify --target=bun-darwin-x64 --outfile dist/proxmux-darwin-x64",
    "build:linux-x64": "bun build ./src/index.tsx --compile --minify --target=bun-linux-x64 --outfile dist/proxmux-linux-x64",
    "build:linux-x64-baseline": "bun build ./src/index.tsx --compile --minify --target=bun-linux-x64-baseline --outfile dist/proxmux-linux-x64-baseline",
    "build:linux-arm64": "bun build ./src/index.tsx --compile --minify --target=bun-linux-arm64 --outfile dist/proxmux-linux-arm64",
    "build:windows-x64": "bun build ./src/index.tsx --compile --minify --target=bun-windows-x64 --outfile dist/proxmux-windows-x64.exe",
    "build:all": "bun run build:darwin-arm64 && bun run build:darwin-x64 && bun run build:linux-x64 && bun run build:linux-x64-baseline && bun run build:linux-arm64 && bun run build:windows-x64"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "dependencies": {
    "ink": "^5.0.1",
    "proxmux": "^0.2.0",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^18.3.12",
    "react-devtools-core": "^7.0.1",
    "typescript": "^5.7.2"
  }
}
